{
  "name": "wretched",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Isaac Graves",
  "packageManager": "pnpm@7.27.0",
  "scripts": {
    "dev": "vite build --watch",
    "build": "pnpm build:runtime && vite build",
    "build:runtime": "RUNTIME=true vite build"
  },
  "dependencies": {
    "browserslist": "^4.21.5",
    "csstype": "3.1.1",
    "goober": "^2.1.12",
    "lightningcss": "^1.19.0"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vite-plugin-dts": "1.7.2"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "jsx-runtime"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.umd.js",
      "require": "./dist/index.umd.js"
    },
    "./jsx-runtime": {
      "types": "./jsx-runtime/dist/jsx-runtime.d.ts",
      "import": "./jsx-runtime/dist/jsx-runtime.mjs",
      "default": "./jsx-runtime/dist/jsx-runtime.umd.js",
      "require": "./jsx-runtime/dist/jsx-runtime.umd.js"
    }
  }
}
